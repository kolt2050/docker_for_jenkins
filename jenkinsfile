node {
    checkout scm

    docker.withRegistry('https://hub.docker.com') {

        docker.image('kolt2050/jenkins_docker').inside {
           sh 'docker --version' 
           sh 'echo 1234'
        }
    }
    stage('Clone repo') {
        println("HELLO")
        sh 'docker --version'
        sh 'git clone https://github.com/epam/edp-install.git'
        sh 'cd edp-install && ls -la'
    }
}
